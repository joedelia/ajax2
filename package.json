{
  "name": "ajax2",
  "version": "0.0.15",
  "description": "A simple ajax library with optional case converter",
  "main": "lib/index.js",
  "scripts": {
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedelia/ajax2.git"
  },
  "author": "Joe d'Elia",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/joedelia/ajax2/issues"
  },
  "homepage": "https://github.com/joedelia/ajax2#readme",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "case-converter": "^1.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^7.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-notify": "^0.4.5",
    "load-grunt-tasks": "^3.5.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none",
          "varsIgnorePattern": "^_"
        }
      ],
      "one-var": [
        "warn",
        {
          "uninitialized": "always"
        }
      ],
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-new": 0,
      "no-prototype-builtins": 0,
      "no-loop-func": 0,
      "no-alert": 0,
      "indent": [
        "error",
        2
      ]
    },
    "ignores": [
      "Gruntfile.js"
    ]
  }
}
